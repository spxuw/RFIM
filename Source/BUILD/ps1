set terminal postscript eps enhanced color 
set size 0.6,0.6
set xlabel 'H'  font 'Times, 20'         
set ylabel 'M'  font 'Times, 20'        
set yrange [-1.2:1.2]      
set xrange [-5:5]      
                       
set output './/data/MH-D2-L20-R2.000-dist1-seed1.eps'     
set nolabel
plot './/data/MH-D2-L20-R2.000-dist1-seed1' u 1:2 notitle w l
